<!DOCTYPE html>
<html ng-app="log-app">
<head>
    <title></title>
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    <link href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.2/toastr.min.css" rel="stylesheet">
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="//rawgit.com/esvit/ng-table/master/dist/ng-table.min.css" rel="stylesheet">
    @Styles.Render("~/bundles/css")
    <base href="/">
</head>
<body>
    <div id="wrapper">
        <div id="top">
            <div class="container">
                <div class="main-logo"><a href="/">Dashboard BNE </a></div>
                <div class="profile">
                    <ul>
                        <li>
                            <a href="http://dashboard.bne.com.br/log" target="_blank">
                                <div class="user-info">
                                    Log de Erro
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="jobs">
                                <div class="user-info">
                                    Vagas
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="http://dashboard.bne.com.br/hangfire/dash" target="_blank">
                                <div class="user-info">
                                    Hangfire
                                </div>
                            </a>
                        </li>
                        <li class="user">
                            <a href="http://www.bne.com.br" target="_blank">
                                <div class="user-info">
                                    www.bne.com.br
                                </div>
                            </a>
                        </li>

                    </ul>
                </div>
            </div>
            <!-- /container -->
        </div>
        <!-- /top -->
        <div id="main">
            <div class="container" id="main-container">
                @RenderBody()
            </div>
        </div>
        <!-- /main -->
    </div>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.2/toastr.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>
    <script src="//rawgit.com/esvit/ng-table/master/dist/ng-table.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery.easytabs/3.2.0/jquery.easytabs.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/0.14.3/ui-bootstrap-tpls.min.js"></script>

    @Scripts.Render("~/bundles/jquerysignalr")
    <script src="~/signalr/hubs"></script>
    @Scripts.Render("~/bundles/base")
</body>
</html>
