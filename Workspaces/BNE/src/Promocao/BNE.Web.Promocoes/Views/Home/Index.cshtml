@{
    ViewBag.Title = "Bne - Indicação";
}


@model BNE.Web.Promocoes.Models.Cadastro

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="The Page Description">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />

    <title> BNE - Indicação</title>
    <link rel="stylesheet" href="/css/layers.min.css" media="screen">
    <link rel="stylesheet" href="/css/font-awesome.min.css" media="screen">
    <link rel="stylesheet" href="/style.css" media="screen">
    <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700|Open+Sans:400italic,700italic,400,700' rel='stylesheet' type='text/css'>
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" type="image/ico" href="favicon2.ico">
    <link rel="icon" href="favicon2.ico">
    <link rel="apple-touch-icon" href="img/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="76x76" href="img/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="120x120" href="img/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="152x152" href="img/apple-touch-icon-152x152.png">

    <script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <script src="//ajax.microsoft.com/ajax/jquery.validate/1.14.0/jquery.validate.min.js"></script>
    <script src="//ajax.microsoft.com/ajax/jquery.validate/1.14.0/additional-methods.min.js"></script>
</head>

<body class="page">
    <div id="fb-root"></div>
    <script>
(function (d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/pt_BR/sdk.js#xfbml=1&version=v2.5";
    fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));
    
  

    </script>
    <script type="text/javascript">
    $(document).ready(function () {
        ConfigurarCampos();
    });
    </script>



    <header role="banner" class="transparent light">
        <div class="row">
            <div class="nav-inner row-content buffer-left buffer-right even clear-after">
                <div id="brand"> 
                    <h1 class="reset">BNE - Banco Nacional de Empregos</h1>
                </div><!-- brand -->
                <a id="menu-toggle" href="#"><i class="fa fa-bars fa-lg"></i></a>
                <nav>
                    <ul class="reset" role="navigation">
                        <li class="menu-item"><a href="http://www.bne.com.br/?utm_source=Hotsite-BH&utm_medium=hotsite&utm_campaign=IndicacaoBH">Ir para o bne.com.br</a></li>
                    </ul>
                </nav>
            </div><!-- row-content -->
        </div><!-- row -->
    </header>

    <main role="main">
        <div id="intro-wrap">
            <div id="intro" class=" darken" data-autoplay="5000" data-navigation="true" data-pagination="true" data-transition="fadeUp">
                <div class="intro-item" style="background-image: url(http://promocoes.bne.com.br/img/bg-belohorizonte.jpg);">
                    <div class="caption">
                        <h2>Olá, você foi indicado por um amigo <br>para usar o BNE</h2>
                        @*<h3 style="color:#fff">Maior site de empregos do Sul e Sudeste do Brasil</h3>*@

                    </div><!-- caption -->
                </div>
            </div><!-- intro -->
        </div><!-- intro-wrap -->

        <div id="main">

            <section class="row section">
                <div class="row-content buffer even clear-after">
                    <div class="section-title"><h3>Alcance de vagas no Brasil</h3></div>
                    <p class="centertxt">Essa é a porcentagem de vagas no BNE distribuído pelas principais regiões do Brasil.</p>
                    <div class="chart" data-percent="57" data-bar-color="#ffce54" data-track-color="#eeeeee" data-animate="2500">
                        <div class="chart-content">
                            <div class="percent"></div>
                            <div class="chart-title">Sudeste</div>
                        </div><!-- chart-content -->
                    </div><!-- chart -->
                    <div class="chart" data-percent="24" data-bar-color="#fd685b" data-track-color="#eeeeee" data-animate="2000">
                        <div class="chart-content">
                            <div class="percent"></div>
                            <div class="chart-title">Sul</div>
                        </div><!-- chart-content -->
                    </div><!-- chart -->
                    <div class="chart" data-percent="13" data-bar-color="#ac92ec" data-track-color="#eeeeee" data-animate="3500">
                        <div class="chart-content">
                            <div class="percent"></div>
                            <div class="chart-title">Nordeste/Norte</div>
                        </div><!-- chart-content -->
                    </div><!-- chart -->
                    <div class="chart" data-percent="6" data-bar-color="#a0d468" data-track-color="#eeeeee" data-animate="3000">
                        <div class="chart-content">
                            <div class="percent"></div>
                            <div class="chart-title">Centro Oeste</div>
                        </div><!-- chart-content -->
                    </div><!-- chart -->

                </div>
            </section>
            <section class="row section call-to-action" style="background-color:#4FC1E9; color:#fff; font-size:26px">
                <div class="row-content buffer even">
                    <p>Depoimentos</p>
                </div>
            </section>
            <section class="row section text-light" style="background-color:#4FC1E9;">
                <div class="row-content buffer even clear-after">
                    <div class="testimonial-slider centertxt" data-autoplay="8000" data-pagination="true" data-transition="fade" data-autoheight="false">
                        <div class="quote">
                            <p>"Olá. Gostaria de contar que consegui um emprego através deste canal. Exatamente o emprego que tanto sonhava. Hoje sou enfermeira auditora de um grande hospital na região de BH. Muito obrigada."</p>
                            <div class="author">Simone - Belo Horizonte/MG</div>
                        </div><!-- quote -->
                        <div class="quote">
                            <p>"Quero agradecer a toda a equipe do BNE pela atenção e colaboração, graças a vocês já estou novamente no mercado de trabalho. Obrigada :)"</p>
                            <div class="author">Flavia - Belo Horizonte/MG</div>
                        </div><!-- quote -->
                        <div class="quote">
                            <p>"Em maio de 2015, através do site BNE, consegui várias entrevistas de emprego. Inclusive fui contratada e tive que cancelar algumas entrevistas."</p>
                            <div class="author">Gisele - Curitiba/PR</div>
                        </div><!-- quote -->
                        <div class="quote">
                            <p>"Vocês estão de parabéns pelo serviço e atendimento... mas tive que cancelar meu currículo pois consegui uma vaga de interesse. Obrigada!"</p>
                            <div class="author">Alyne - Betim/MG</div>
                        </div><!-- quote -->
                        <div class="quote">
                            <p>"Em pouco tempo consegui emprego, estou muito satisfeito e feliz, obrigado!"</p>
                            <div class="author">Anderson - Novo Hamburgo/RS</div>
                        </div><!-- quote -->
                    </div><!-- testimonial-slider -->
                </div>
            </section>



            <section class="row section greyish ">
                <div class="row-content buffer even clear-after">
                    <div class="section-title "><h3>Faça o seu cadastro agora e candidate-se nas vagas</h3></div>
                    <div class="column two"> </div>
                    <div class="column eight">
                        <form id="contact-form" class="contact-section" action="">
                            @using (Html.BeginForm()){
                                @Html.ValidationSummary(true)
                                <fieldset>
                                  
                                    @Html.HiddenFor(m => m.idCidade, new { @value = "" })
                                    @Html.HiddenFor(m => m.idFuncao, new { @value = "" })

                                    <div id="avisoNome" style="display: none;"></div>
                                    <span class="pre-input"><i class="icon icon-user"></i></span>
                                    @Html.TextBoxFor(m => m.nome, new { @class = "name plain buffer", placeholder = "Nome Completo", name = "txtNome", onBlur="valNome();" })

                                    <div id="avisoCPF" style="display: none;"></div>
                                    <span class="pre-input"><i class="icon icon-user"></i></span>
                                    @Html.TextBoxFor(m => m.cpf, new { @class = "name plain buffer", placeholder = "CPF", id = "txtCPF", onKeyUp = "CPFMask()", onBlur = "valCPF()" })

                                    <div id="avisoDataNascimento" style="display: none;"></div>
                                    <span class="pre-input"><i class="icon icon-user"></i></span>
                                    @Html.TextBoxFor(m => m.datanascimento, new { @class = "name plain buffer", placeholder = "Data de Nascimento", onKeyUp="dataMask()", onBlur = "valDataNascimento()", id = "txtDataNascimento", name = "txtDataNascimento" })


                                    <div id="avisoEmail" style="display: none;"></div>
                                    <span class="pre-input"><i class="icon icon-email"></i></span>
                                    @Html.TextBoxFor(m => m.email, new { @class = "email plain buffer", placeholder = "Email", onChange ="valEmail();" })

                                    <div id="avisoCelular" style="display: none;"></div>
                                    <span class="pre-input"><i class="icon icon-email"></i></span>
                                    @Html.TextBoxFor(m => m.celular, new { @class = "name plain buffer", placeholder = "Celular", onChange = "valCelular();" })

                                    <div id="avisoFuncao" style="display: none;"></div>
                                    <span class="pre-input"><i class="icon icon-money"></i></span>
                                    @Html.TextBoxFor(m => m.funcao, new { @class = "cidade plain buffer", placeholder = "Função Desejada" })

                                    <div id="avisoPretensao" style="display: none;"></div>
                                    <span class="pre-input"><i class="icon icon-money"></i></span>
                                    @Html.TextBoxFor(m => m.pretensao, new { @class = "name plain buffer", placeholder = "Pretensão salarial", onChange = "valPretensaoMaiorqueSalarioMinimo();" })

                                    <div id="avisoCidade"  style="display: none;"></div>
                                    <span class="pre-input"><i class="icon icon-pin"></i></span>
                                    @Html.TextBoxFor(m => m.cidade, new { @class = "funcao plain buffer", placeholder = "Cidade" })

                                    <div id="avisoSexo" style="display: none;"></div>
                                    <input type="radio" name="sexo" id="sexoMasculino" value="1" />
                                    <label for="sexoMasculino">Masculino</label>
                                    <input type="radio" name="sexo" id="sexoFeminino" value="2" />
                                    <label for="sexoFeminino">Feminino</label>
                                    <p>
                                        <input class=" plain button blue" id="btnCadastrar" type="button" onclick="validarCadastro();" value="Cadastrar">
                                    </p>
                                </fieldset>
                            }
                            <div class="contact-section" >
                            
                            </div>
                        </form>
                        <div id="success"></div>
                    </div>
                    <div class="column two last"> </div>
                </div>
            </section>

            <section class="row section call-to-action" style="background-color:#4FC1E9">
                <div id="DivInfosBNE">
                    BNE - Banco Nacional de Empregos   |  CNPJ 82.344.425/0001-82   |  0800 41 2400
                </div>
            </section>

        </div><!-- id-main -->
    </main><!-- main -->
   
   
    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="/js/plugins.js"></script>
    <script src="/js/beetle.js"></script>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Styles.Render("~/Content/themes/base/css")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/js/Sugest/sugest")
    @Scripts.Render("~/bundles/scripts/components")
}


    <script type="text/javascript">
        $('#nome').val(getParameterByName('nome'));
        $('#email').val(getParameterByName('email'));
    </script>
   
</body>

</html>




