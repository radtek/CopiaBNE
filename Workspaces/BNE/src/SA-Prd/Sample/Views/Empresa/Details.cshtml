@model AdminLTE_Application.VWEmpresa
@using Microsoft.AspNet.Identity
@using System.Security.Claims
@{
    ViewBag.Title = "CRM Detalhes Empresa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Styles.Render("~/Content/css/datetimepickers.css")
<link href="~/Content/css/datepicker/bootstrap-datetimepicker.min.css" rel="stylesheet" />
<aside class="right-side">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Painel Empresa
            <small>Detalhes</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="~/Views/Home/"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Lista de Empresas</li>
            <li class="active">Empresa</li>
        </ol>
    </section>

   
            <!-- Main content -->
            <section class="content">
                <!-- Dados -->
                <div class="row">
                    <section class="col-lg-5">
                        <div class="box box-success box-group">
                            <div class="box-header">
                                <div class="col-lg-12">
                                    <h4 class="box-title">@Html.DisplayFor(model => model.Raz_Social)</h4>
                                    <div>
                                        <br><br><br>
                                        <b>CNPJ:</b> @Html.DisplayFor(model => model.Num_CNPJ).ToString().Replace(",00","")<br>
                                        <b>Fone comercial:</b> (@Html.DisplayFor(model => model.Num_DDD_Comercial)) @Html.DisplayFor(model => model.Num_Comercial)<br>
                                        <b>Localidade:</b> @Html.DisplayFor(model => model.Nme_Cidade) - @Html.DisplayFor(model => model.Sig_Estado)<br>
                                        <b>Natureza jurídica:</b> @Html.DisplayFor(model => model.Des_Natureza_Juridica)<br>
                                        <b>CNAE:</b> @Html.DisplayFor(model => model.Des_CNAE_Sub_Classe)<br>

                                        @if (!(Model.Qtd_Funcionarios == null))
                                        {
                                            <b>Qtd Funcionários:</b> @Html.DisplayFor(model => model.Qtd_Funcionarios)<br>
                                        }
                                        <b>Data de cadastro:</b> @Html.DisplayFor(model => model.Dta_Cadastro)<br>
                                        @if (!string.IsNullOrEmpty(Model.End_Site))
                                        {
                                            <b>Site:</b> <a href="@Html.DisplayFor(model => model.End_Site)">@Html.DisplayFor(model => model.End_Site)</a><br>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.Des_Plano))
                                        {
                                            <b>Plano: </b>@Html.DisplayFor(model => model.Des_Plano)<br>
                                            <b>Período: </b>@Html.DisplayFor(model => model.Dta_Inicio_Plano) <span> até </span>@Html.DisplayFor(model => model.Dta_Fim_plano)<br>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.Des_URL))
                                        {
                                            <b>STC: </b><a href="@Html.DisplayFor(model => model.Des_URL)">@Html.DisplayFor(model => model.Des_URL)</a><br>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.Nme_Usuario))
                                        {
                                            <b>Usuário responsável: </b>@Html.DisplayFor(model => model.Nme_Usuario)<br>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.Num_Comercial_Usuario))
                                        {
                                            <b>Função: </b>@Html.DisplayFor(model => model.Des_Funcao_Usuario)<br>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.Num_Comercial_Usuario))
                                        {
                                            <b>Telefone: </b><a href="tel:@Html.DisplayFor(model => model.Num_DDD_Comercial_Usuario)@Html.DisplayFor(model => model.Num_Comercial_Usuario)">@Html.DisplayFor(model => model.Num_DDD_Comercial_Usuario)  @Html.DisplayFor(model => model.Num_Comercial_Usuario)</a><br>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.Nme_Vendedor))
                                        {
                                            <b>Vendedor: </b>@Html.DisplayFor(model => model.Nme_Vendedor)<span> - </span>@Html.DisplayFor(model => model.Dta_Inicio_Carteira)<span> até </span> @Html.DisplayFor(model => model.Dta_Fim_Carteira)<br>
                                        }
                                        <!--TODO pegar perfil do ClaimsPrincipal-->
                                        @if ((Model.Eml_Vendedor == User.Identity.GetUserName()) ||
                                            ( User.Identity.GetUserName() == "adrianogoncalves@bne.com.br"))/*ClaimsPrincipal.Current.FindFirst("tipoVendedor").ToString()=="6"*/
                                        {
                                            <div id="transferencia">
                                                <form method="get" action="~/Transferir/" class="sidebar-form">
                                                    <div class="input-group">
                                                        <select type="hidden" class="form-control" id="vendedor" name="vendedor">
                                                            <option>option 1</option>
                                                            <option>option 2</option>
                                                            <option>option 3</option>
                                                            <option>option 4</option>
                                                            <option>option 5</option>
                                                        </select>
                                                        <input type="hidden" name="CNPJ" id="CNPJ" value="@Html.DisplayFor(model => model.Num_CNPJ)" />
                                                        <span class="input-group-btn">
                                                            <button type='submit' class="btn btn-flat"><i class=" fa fa-exchange"></i></button>
                                                        </span>
                                                    </div>
                                                </form>
                                            </div>
                                        }
                                       
                                    </div>
                                </div>
                            </div>
                        </div>
                            <!--</section>
            <section class="col-lg-5 connectedSortable ui-sortable">-->
                            
                        <!-- Chat box -->
                        <div class="box box-success box-group">
                            <div class="box-header">
                                <i class="fa fa-comments-o"></i>
                                <h3 class="box-title">@Html.ActionLink("CRM", "Details", new { controller = "HistoricoCRM", Id = Model.Num_CNPJ })</h3>
                                <div class="box-tools pull-right">
                                    <button class="btn btn-default btn-sm" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                    <button class="btn btn-default btn-sm" data-widget="remove"><i class="fa fa-times"></i></button>
                                </div>
                            </div>
                            <div class="box-footer">
                                @using (Html.BeginForm("AddAtendimento", "Empresa", FormMethod.Post))
                                {
                                    <div class="form-group">
                                        <div class="input-group">
                                                @Html.TextBox("DataAtendimento", ViewBag.DataAtendimento as string, new { @class = "form_datetime form-control", @id = "DataAtendimento", @readonly = ""})
                                                <div class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </div>
                                            </div>
                                      
                                        <div class="input-group">
                                            @Html.TextBox("Msg", ViewBag.Msg as string, new { @class = "form-control type='text'" })
                                            <div class="input-group-btn">
                                                <button class="btn btn-success" type="submit"><i class="fa fa-plus"></i></button>
                                            </div>
                                        </div>
                                        <div>
                                            @Html.HiddenFor(x => Model.Num_CNPJ, ViewBag.Num_CNPJ as string)
                                        </div>
                                    </div>
                                }
                            </div>
                            <div id="crm" class="box-footer no-border chat">
                            </div><!-- /.chat -->

                        </div><!-- /.box -->
                    <!--Empresas do Ramo-->
