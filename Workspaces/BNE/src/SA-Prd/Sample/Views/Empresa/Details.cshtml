                                                <div class="box-body chart-responsive">
                                                    <div class="box-body table-responsive no-padding">
                                                        <table id="cvsNaLocalidade" class="table table-hover">
                                                            <tbody>
                                                                <tr>
                                                                    <th>Cidade</th>
                                                                    <th>Total</th>
                                                                </tr>
                                                            </tbody>
